{
    "version": 3,
    "vendor": {
        "conan": {}
    },
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 15,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "conan-debug",
            "displayName": "'conan-debug' config",
            "description": "'conan-debug' configure using 'Ninja' generator",
            "generator": "Ninja",
            "cacheVariables": {
                "CMAKE_POLICY_DEFAULT_CMP0091": "NEW",
                "CMAKE_BUILD_TYPE": "Debug"
            },
            "toolchainFile": "${sourceDir}/../build/conan_toolchain.cmake",
            "binaryDir": "${sourceDir}/../build-debug"
        },
        {
            "name": "conan-release",
            "displayName": "'conan-release' config",
            "description": "'conan-release' configure using 'Ninja' generator",
            "generator": "Ninja",
            "cacheVariables": {
                "CMAKE_POLICY_DEFAULT_CMP0091": "NEW",
                "CMAKE_BUILD_TYPE": "Release"
            },
            "toolchainFile": "${sourceDir}/../build/conan_toolchain.cmake",
            "binaryDir": "${sourceDir}/../build"
        }
    ],
    "buildPresets": [
        {
            "name": "conan-debug",
            "configurePreset": "conan-debug",
            "jobs": 11
        },
        {
            "name": "conan-release",
            "configurePreset": "conan-release",
            "jobs": 11
        }
    ],
    "testPresets": [
        {
            "name": "conan-debug",
            "configurePreset": "conan-debug",
            "execution": {
                "jobs": 11
            }
        },
        {
            "name": "conan-release",
            "configurePreset": "conan-release",
            "execution": {
                "jobs": 11
            }
        }
    ]
}